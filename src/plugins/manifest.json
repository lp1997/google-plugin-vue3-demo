{
    "manifest_version": 2,
    "name": "demo",
    "description": "工具",
    "version": "1.0.0",
    "browser_action": {
        "default_title": "demo",
        "default_icon": "assets/icon48.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/background.js",
            "js/menu.js"
        ]
    },
    "icons": {
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
    },
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "proxy",
        "storage",
        "activeTab",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "css": [
                "css/popup.css"
            ],
            "js": [
                "js/jquery-2.1.3.min.js",
                "js/chunk-vendors.js",
                "js/main.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        "js/main.js"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}